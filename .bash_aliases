# nvim has been added to PATH
# wrap q with rlwrap
alias q='rlwrap -r q'
alias ls='ls --color=auto'
alias ll='ls -lrthA'
# read a pdf
alias pdf='evince'
# no nonsense R
alias R='R --no-save --no-restore --quiet'
# link r to radian
alias r='radian'
# link vim to nvim
alias vim='nvim'
alias vi='nvim'

